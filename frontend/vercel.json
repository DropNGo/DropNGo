{
  "version": 2,
  "name": "DropNGo",
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "cleanUrls": true,
  "rewrites": [
    { "source": "/agb", "destination": "/agb.html" },
    { "source": "/cookie-banner", "destination": "/cookie-banner.html" },
    { "source": "/datenschutz", "destination": "/datenschutz.html" },
    { "source": "/impressum", "destination": "/impressum.html" },
    { "source": "/produkte", "destination": "/products.html" },
    { "source": "/produkt1", "destination": "/produkt-detail1.html" },
    { "source": "/produkt2", "destination": "/produkt-detail2.html" },
    { "source": "/produkt3", "destination": "/produkt-detail3.html" },
    { "source": "/support", "destination": "/support-formular.html" },
    { "source": "/widerrufsbelehrung", "destination": "/widerrufsbelehrung.html" },
    { "source": "/login", "destination": "/login.html" },
    { "source": "/reset", "destination": "/reset.html" },
    { "source": "/account", "destination": "/account.html" },
    { "source": "/login", "destination": "/login.html" },
    { "source": "/reset", "destination": "/reset.html" },
    { "source": "/mobile-template", "destination": "/", "permanent": true },
    { "source": "/mobile-template.html", "destination": "/", "permanent": true }
  ]
}
